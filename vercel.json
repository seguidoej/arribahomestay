{
  "version": 2,
  "builds": [
    {
      "src": "index.html",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/logo/(.*)",
      "dest": "/public/logo/$1"
    },
    {
      "src": "/hero/(.*)",
      "dest": "/public/hero/$1"
    },
    {
      "src": "/gallery/(.*)",
      "dest": "/public/gallery/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}
